{
  "name": "@blockv/sdk",
  "version": "4.0.0-alpha.17",
  "description": "Allows web apps to display and interact with vatoms.",
  "main": "lib/index-node.js",
  "browser": "src/index.js",
  "scripts": {
    "build-node": "babel --config-file=./node.babelrc -d lib/ src/",
    "build-browser": "webpack --mode=production",
    "build-faces-browser": "webpack --mode=production --config ./face/webpack.config.js",
    "build-dev": "webpack --mode=development --config ./face/webpack.config.js",
    "build": "concurrently \"npm run build-node\" \"npm run build-browser\" \"npm run build-faces-browser\"",
    "prepare": "npm run build",
    "mocha": "mocha"
  },
  "keywords": [
    "vatom",
    "blockv",
    "browser",
    "web",
    "html"
  ],
  "files": [
    "src/",
    "face/",
    "lib/",
    "dist/"
  ],
  "author": "Blockv",
  "license": "BLOCKv AG",
  "source": true,
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-standard": "^4.1.0",
    "file-loader": "^5.1.0",
    "postcss-loader": "~3.0.0",
    "sass-loader": "^8.0.2",
    "should": "^13.2.3",
    "style-loader": "^1.3.0",
    "unexpected": "^11.15.1",
    "url-loader": "^3.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "better-assert": "^1.0.2",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "cross-fetch": "^3.1.4",
    "dotenv": "^8.6.0",
    "css-element-queries": "^1.2.3",
    "expect.js": "^0.3.1",
    "geolib": "^3.3.3",
    "jwt-decode": "^2.2.0",
    "latlon-geohash": "^2.0.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "lzutf8": "^0.5.8",
    "url-parse": "^1.5.3"
  },
  "repository": "github:BLOCKvIO/web-sdk",
  "jest": {
    "bail": true
  },
  "standard": {
    "globals": [
      "WebSocket"
    ]
  }
}
